<launch>

    <param name="robot_description" command="$(find xacro)/xacro $(find urdf_gazebo)/xacro/myrobot.urdf.xacro" />

    <include file="$(find gazebo_ros)/launch/empty_world.launch" >
       <arg name="world_name" value="$(find urdf_gazebo)/worlds/box_house.world" />
    </include>

    <node pkg="gazebo_ros" type="spawn_model" name="model" args="-urdf -model robotenv -param robot_description"  />
   
</launch>